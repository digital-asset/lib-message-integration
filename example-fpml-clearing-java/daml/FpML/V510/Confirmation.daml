    = TriggerTimeTypeEnum_Closing ()
      -- ^ The close of trading on a day would be considered for
      --   valuation.
    | TriggerTimeTypeEnum_Anytime ()
      -- ^ At any time during the Knock Determination period
      --   (continuous barrier).
