    | GasProductTypeEnum_Isobutane ()
      -- ^ Iso-Butane (C4H10)
    | GasProductTypeEnum_Methane ()
      -- ^ Methane (CH4)
    | GasProductTypeEnum_Naphtha ()
      -- ^ Naphtha (C5  C12)
    | GasProductTypeEnum_NaturalGas ()
